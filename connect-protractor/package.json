{
  "name": "connect-protractor",
  "version": "1.1.1",
  "description": "load the config from external sources and run protractor. designed to be run from protractor flake.",
  "author": "Richard Vowles <richard@bluetrainsoftware.com>",
  "main": "./target/app/config-loader.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ClearPointNZ/connect-node.git"
  },
  "keywords": [
    "config",
    "protractor",
    "pookums",
    "properties",
    "yaml"
  ],
  "scripts": {
    "build": "yarn run tsc",
    "build:watch": "yarn run build -- -- -w",
    "test": "yarn run mocha -- --opts mocha.opts",
    "test:watch": "yarn run mocha --opts mocha.opts --watch",
    "mocha": "node ./node_modules/mocha/bin/mocha",
    "tsc": "node ./node_modules/typescript/bin/tsc",
    "lint": "node ./node_modules/tslint/bin/tslint -r ./node_modules/tslint-microsoft-contrib/ \"app/**/*.ts\" \"spec/**/*.ts\" -e \"app/generated-code/**/*\""
  },
  "dependencies": {
    "connect-config-loader": "file:.yalc/connect-config-loader",
    "connect-logger": "file:.yalc/connect-logger",
    "connect-vault-initializer": "file:.yalc/connect-vault-initializer",
    "cucumber": "3.0.4",
    "protractor": "^5.3.0",
    "protractor-cucumber-framework": "4.0.0"
  },
  "engines": {
    "node": ">=8.1.0"
  },
  "devDependencies": {
    "connect-build": "file:.yalc/connect-build"
  }
}
